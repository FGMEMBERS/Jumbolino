<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
	<path>radar.ac</path>
 
	<animation>
		<type>scale</type>
		<x-offset>0.2</x-offset>
		<y-offset>0.2</y-offset>
		<z-offset>0.1</z-offset>
	</animation>

	<animation>
		<type>material</type>
