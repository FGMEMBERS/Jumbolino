<!-- APU -->

		<engine>
   <name>APU</name>
   		<path>Aircraft/Jumbolino/Sounds/apu.wav</path>
   		<mode>once</mode>
	<condition>
		<not>
	<property>sim/current-view/internal</property>
		</not>
        <property>/controls/electric/APU-generator</property>
	</condition>  
		<position>
			<x>0</x>
			<y>0</y>
			<z>0</z>
		</position>      
   	  <volume>
        <property>engines/engine/n1</property>
        <factor> 0.09 </factor>
        <min> 0.0 </min>
        <max> 0.4 </max>
      </volume>
      <pitch>
        <property>engines/engine/n1</property>
        <factor> 0.01 </factor>
        <min> 0.0 </min>
        <max> 1.0 </max>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>


