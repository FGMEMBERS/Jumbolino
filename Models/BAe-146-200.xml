	<!-- ############## -->
	<!-- Gear Animation -->
	<!-- ############## -->

	<!--       Left Gear      -->
	
	<animation>
		<name>left_gear</name>
		<object-name>Tires.MainGear.Left</object-name>
		<object-name>Wheels.MainGear.Left</object-name>
		<object-name>MainGear.StroutLeft</object-name>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>left_gear</object-name>
		<property>gear/gear[1]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>95</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>6.407</x-m>
			<y-m>-2.150</y-m>
			<z-m>-1.725</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- Right Gear -->
	
	<animation>
		<name>right_gear</name>
		<object-name>Tires.MainGear.Right</object-name>
		<object-name>Wheels.MainGear.Right</object-name>
		<object-name>MainGear.StroutRight</object-name>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>right_gear</object-name>
		<property>gear/gear[2]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>95</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>6.407</x-m>
			<y-m> 2.150</y-m>
			<z-m>-1.725</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>MainGear.DoorLeft</object-name>
		<property>gear/gear[2]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>120</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>6.507</x-m>
			<y-m>-1.460</y-m>
			<z-m>-2.820</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>MainGear.DoorRight</object-name>
		<property>gear/gear[2]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>120</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>6.507</x-m>
			<y-m> 1.460</y-m>
			<z-m>-2.820</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- Nose Gear -->
	
	<animation>
		<name>nose_gear</name>
		<object-name>Tires.NoseGear</object-name>
		<object-name>Wheels.NoseGear</object-name>
		<object-name>NoseGear.Strout</object-name>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>nose_gear</object-name>
		<property>gear/gear[0]/position-norm</property>

		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>90</dep>
			</entry>
			<entry>
				<ind>0.2</ind>
				<dep>45</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>-10.250</x-m>
			<y-m>0.0</y-m>
			<z-m>-3.050</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>NoseGear.Door</object-name>
		<property>gear/gear[0]/position-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>111</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>-9.925</x-m>
			<y-m>-0.750</y-m>
			<z-m>-3.330</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- Gear disapears when raised -->

	<animation>
		<type>select</type>
		<object-name>right_gear</object-name>
		<condition>
			<property>gear/gear[0]/position-norm</property>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>nose_gear</object-name>
		<condition>
			<property>gear/gear[0]/position-norm</property>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>left_gear</object-name>
		<condition>
			<property>gear/gear[0]/position-norm</property>
		</condition>
	</animation>

